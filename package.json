{
  "name": "lilith",
  "version": "1.0.0",
  "description": "Quick valuations and cost estimates",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/assets/img/icons/icon64.icns --prune=true --out=release-builds",
    "dist": "build"
  },
  "author": "makoto <makoto.kontakt@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1",
    "minimist": "^1.2.5"
  },
  "dependencies": {
    "browserify": "^16.5.0",
    "cargodb": "^2.0.0",
    "electron-builder": "^22.4.1",
    "electron-store": "^5.1.1",
    "localStorage": "^1.0.4",
    "mathjs": "^6.6.1",
    "os": "^0.1.1",
    "popmotion": "^8.7.1"
  },
  "engines": {
    "node": "12.x"
  },
  "build": {
    "appId": "com.makoto.Lilith",
    "productName": "Lilith",
    "linux": {
      "category": "Office",
      "icon": "assets/img/icons",
      "packageCategory": "office",
      "maintainer": "makoto <makoto.kontakt@gmail.com>",
      "target": [
        "deb"
      ]
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3"
      ]
    }
  }
}
